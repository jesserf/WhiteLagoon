@model IEnumerable<WhiteLagoon.Domain.Entities.Villa>

<h1>Villas</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Occupancy</th>
            <th>Square Feet</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ojb in Model)
        {
            <tr>
                <td>@ojb.Name</td>
                <td>@ojb.Description</td>
                <td>@ojb.Occupancy</td>
                <td>@ojb.Sqft</td>
                <td>@ojb.Price</td>
            </tr>
        }
    </tbody>
</table>
